<table>
  <thead>
    <tr>
      <th>ジャンル一覧</th>
    </tr>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
      <tr>
        <td>
          <%= link_to scores_path(genre_id: genre.id) do %>
            <%= genre.genre_name %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>